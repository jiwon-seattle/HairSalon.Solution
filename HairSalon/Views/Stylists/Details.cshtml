@{
  Layout = "_Layout";
}

@model HairSalon.Models.Stylist

<div class="container">
  <h2>Stylist Details</h2>
  <h4>Stylist Name : @Html.DisplayFor(model => model.StylistName)</h4>
  <h4>Main Skill : @Html.DisplayFor(model => model.MainSkill)</h4>
  <h4>Previous Company : @Html.DisplayFor(model => model.ExCompany)</h4>
  <h4>Work Experience : @Html.DisplayFor(model => model.WorkExperience)</h4>
  <h4>Start Date : @Html.DisplayFor(model => model.HiredDate)</h4>
</div>