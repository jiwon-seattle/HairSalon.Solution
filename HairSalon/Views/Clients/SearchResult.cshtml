@{
  Layout = "_Layout";
}


@using HairSalon.Models;
@model List<HairSalon.Models.Client>

<div class="clientSearchResult">
  <h2><strong>Clients</strong></h2>

  <div id="clientSearchLists">
    @if (Model.Count == 0)
    {
      <h3>No clients have been found</h3>

    }
    @foreach (Client client in Model)
    {
      <p>@Html.ActionLink($"{client.ClientName}", "Details", new {id = client.ClientId})</p>
    }
  </div>

</div>